{
    "name": "cli",
    "description": "CLI tool for interacting with ServerSinc 👩‍🚀",
    "version": "0.0.1",
    "engines": {
        "node": ">=18.16.0"
    },
    "main": "build/index.js",
    "type": "module",
    "files": [
        "build/bin",
        "build/src",
        "build/index.d.ts",
        "build/index.js"
    ],
    "bin": {
        "sinc": "build/bin/run.js"
    },
    "exports": {
        ".": "./build/index.js"
    },
    "scripts": {
        "clean": "del-cli build",
        "compile": "npm run clean && tsc",
        "build": "npm run compile",
        "version": "npm run build",
        "prepublishOnly": "npm run build",
        "typecheck": "tsc --noEmit"
    },
    "devDependencies": {
        "@adonisjs/eslint-config": "^1.1.7",
        "@adonisjs/prettier-config": "^1.1.7",
        "@adonisjs/tsconfig": "^1.1.7",
        "@types/fs-extra": "^11.0.1",
        "@types/node": "^20.4.1",
        "del-cli": "^5.0.0",
        "eslint": "^8.44.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6"
    },
    "dependencies": {
        "@adonisjs/ace": "12.3.1-6",
        "axios": "^1.4.0",
        "fs-extra": "^11.1.1"
    },
    "author": "dmdboi",
    "license": "MIT",
    "homepage": "https://github.com/serversinc/cli#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/serversinc/cli.git"
    },
    "bugs": {
        "url": "https://github.com/serversinc/cli/issues"
    }
}